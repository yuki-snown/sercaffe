<p id="notice"><%= notice %></p>

<h1>Stores</h1>

<table>
  <thead>
    <tr>
      <th>Ranking</th>
      <th>Rating</th>
      <th>Company</th>
      <th>Image</th>
      <th>Weekdays start</th>
      <th>Time</th>
      <th>Weekdays end</th>
      <th>Holidays start</th>
      <th>Holidays end</th>
      <th>Size m</th>
      <th>Integer</th>
      <th>Size l</th>
      <th>Walk time</th>
      <th>Length</th>
      <th>Power</th>
      <th>Wifi</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.ranking_id %></td>
        <td><%= store.rating %></td>
        <td><%= store.company_id %></td>
        <td><%= store.image %></td>
        <td><%= store.weekdays_start %></td>
        <td><%= store.time %></td>
        <td><%= store.weekdays_end %></td>
        <td><%= store.holidays_start %></td>
        <td><%= store.holidays_end %></td>
        <td><%= store.size_m %></td>
        <td><%= store.integer %></td>
        <td><%= store.size_l %></td>
        <td><%= store.walk_time %></td>
        <td><%= store.length %></td>
        <td><%= store.power %></td>
        <td><%= store.wifi %></td>
        <td><%= link_to 'Show', store %></td>
        <td><%= link_to 'Edit', edit_store_path(store) %></td>
        <td><%= link_to 'Destroy', store, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store', new_store_path %>
